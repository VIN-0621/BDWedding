package cn.edu.fzu.sm.bdwedding.client;

public class Client1 {
	private int cId;
	private String cName;
	private String cPhone;
	private String cPhototDate;
	private String cPay;
	private String cSort;
	public int getcId() {
		return cId;
	}
	public void setcId(int cId) {
		this.cId = cId;
	}
	public String getcName() {
		return cName;
	}
	public void setcName(String cName) {
		this.cName = cName;
	}
	public String getcPhone() {
		return cPhone;
	}
	public void setcPhone(String cPhone) {
		this.cPhone = cPhone;
	}
	public String getcPhototDate() {
		return cPhototDate;
	}
	public void setcPhototDate(String cPhototDate) {
		this.cPhototDate = cPhototDate;
	}
	public String getcPay() {
		return cPay;
	}
	public void setcPay(String cPay) {
		this.cPay = cPay;
	}
	public String getcSort() {
		return cSort;
	}
	public void setcSort(String cSort) {
		this.cSort = cSort;
	}
	
	
	
}
